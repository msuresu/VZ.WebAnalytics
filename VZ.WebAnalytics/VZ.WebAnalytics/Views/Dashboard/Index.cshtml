@{
    ViewBag.Title = "Index";
}
<div ng-show="IsDashBoard">
    <div class="row">
        <div class="col-lg-12">

        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Dashboard <input class="form-control" placeholder="Enter text" value="{{DomainName}}">
            </h1>
            
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> Site Metrics</h3>
                </div>
                <div class="panel-body">
                    <div id="siteMetrcis" style="height:300px"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">


        <div class="col-lg-3 col-md-6">
            <div class="panel panel-yellow">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            Visits
                        </div>
                        <div class="col-xs-6 text-right">
                            <div class="huge">{{TotalVistCount}}</div>
                        </div>
                    </div>
                </div>

                <div class="panel-footer">
                    <div id="siteMetrcis1" style="height:100px"></div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-yellow">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            Unique Visitors
                        </div>
                        <div class="col-xs-6 text-right">
                            <div class="huge">{{TotalUniqueCount}}</div>
                        </div>
                    </div>
                </div>

                <div class="panel-footer">
                    <div id="siteMetrcis2" style="height:100px"></div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-yellow">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            Page Views
                        </div>
                        <div class="col-xs-6 text-right">
                            <div class="huge">{{TotalViewCount}}</div>
                        </div>
                    </div>
                </div>

                <div class="panel-footer">
                    <div id="siteMetrcis3" style="height:100px"></div>
                </div>

            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="panel panel-yellow">
                <div class="panel-heading">
                    <div class="row">
                        <div class="col-xs-6">
                            Pages Per Visit
                        </div>
                        <div class="col-xs-6 text-right">
                            <div class="huge">{{TotalPerViewViewCount}}</div>
                        </div>
                    </div>
                </div>

                <div class="panel-footer">
                    <div id="siteMetrcis4" style="height:100px"></div>
                </div>

            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-lg-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-long-arrow-right fa-fw"></i>Visitor Types</h3>
                </div>
                <div class="panel-body">
                    <div id="visterTypeBarChart" style="height:250px"></div>

                </div>
            </div>
        </div>

        <div class="col-lg-8">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-money fa-fw"></i>Top Contents</h3>
                </div>
                <div class="panel-body" style="height:280px;overflow-y:auto">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>
                                    <th>Date</th>
                                    <th>Page Path</th>
                                    <th>Page Title</th>
                                    <th>Page Viwes</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="row in TopContents">

                                    <td>{{row.Date}}</td>
                                    <td>{{row.PagePath}}</td>
                                    <td>{{row.PageTitle}}</td>
                                    <td>{{row.PageViwes}}</td>
                                </tr>

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<div id="pageview" ng-show="IsPageView">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Web Pages
            </h1>

        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i>Page Views</h3>
                </div>
                <div class="panel-body">
                    <div id="siteMetrcisPageViews" style="height:300px"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
       
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-money fa-fw"></i>Page Viwes</h3>
                </div>
                <div class="panel-body" >
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>                                    
                                    <th>Page Path</th>
                                    <th>Page Title</th>
                                    <th>Page Viwes</th>
                                    <th>Visits</th>
                                    <th>Unique Page Viwes</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="row in TopContentsAll">
                                                                        
                                    <td>{{row.PagePath}}</td>
                                    <td>{{row.PageTitle}}</td>
                                    <td>{{row.PageViwes}}</td>
                                    <td>{{row.Visits}}</td>
                                    <td>{{row.UniquePageViwes}}</td>
                                </tr>

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="row">

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-money fa-fw"></i>Page Viwes</h3>
                </div>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>
                                    <th>Page Path</th>
                                    <th>Page Title</th>
                                    <th>Page Viwes</th>
                                    <th>Visits</th>
                                    <th>Unique Page Viwes</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="row in TopContentsAll">

                                    <td>{{row.PagePath}}</td>
                                    <td>{{row.PageTitle}}</td>
                                    <td>{{row.PageViwes}}</td>
                                    <td>{{row.Visits}}</td>
                                    <td>{{row.UniquePageViwes}}</td>
                                </tr>

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="row">

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-money fa-fw"></i>Action</h3>
                </div>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>
                                    <th>Page Path</th>
                                    <th>Page Title</th>
                                    <th>Element Type</th>
                                    <th>Element Name</th>                                   
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="row in TopContentsAction">

                                    <td>{{row.PagePath}}</td>
                                    <td>{{row.PageTitle}}</td>
                                    <td>{{row.ElementType}}</td>
                                    <td>{{row.ElementName}}</td>                                    
                                </tr>

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<div id="pagePerformance" ng-show="IsPerformance">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                Page Performance
            </h1>

        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title"><i class="fa fa-money fa-fw"></i>Top Contents</h3>
                </div>
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover table-striped">
                            <thead>
                                <tr>
                                    <th>Page URL</th>
                                    <th>Time Spent(in Secs)</th>
                                    <th>Page Load Time(in Secs)</th>
                                    <th>Browser Type & Version</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="objPer in PagePerformance">
                                    <td>{{objPer.PagePath}}</td>
                                    <td>{{GetDuration(objPer.StartTime,objPer.EndTime)}}</td>
                                    <td>{{objPer.LoadTime}}</td>
                                    <td>{{objPer.BrowserInfo}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>